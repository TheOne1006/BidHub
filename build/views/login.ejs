<% include header %>

  <h1><%-title %></h1>
  <form action="/login" method="post">
    <input type="text" name="username" placeholder="如: 小明" />
    <input type="submit" value="登录" />
  </form>
  <div>
    <h2>用户名列表:</h2>
    <ul>
      <% users.forEach(function(user) { %>
      <li><%=user.username %> - walet <%=user.walet %></li>
      <% }) %>
    </ul>
  </div>
<% include footer %>
