<% include header %>

  <h1><%-title %></h1>
  <hr />
  <h2> user list from Mongodb</h2>
  <% users.forEach(function(user) { %>
    <h3><%=user.username %></h3>
    <p>_id: <%=user._id %></p>
    <p>walet: <%=user.walet %></p>
  <% }) %>
  <hr />
  <h2>product list from Mongodb</h2>
  <% products.forEach(function(product) { %>
    <h3><%=product.name %></h3>
    <p>_id: <%=product._id %></p>
    <p>sold: <%=product.sold %></p>
    <p>listPrice: <%=product.listPrice %></p>
    <p>bidPrice: <%=product.bidPrice %></p>
    <p>ts: <%=product.ts %></p>
  <% }) %>
<% include footer %>
